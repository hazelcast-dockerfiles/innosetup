<launch4jConfig>
    <headerType>console</headerType>
    <outfile>%DIST_DIR%/bin/HazelcastClient.exe</outfile>
    <jar>%DIST_DIR%/lib/hazelcast-%HAZELCAST_VERSION%.jar</jar>
    <dontWrapJar>true</dontWrapJar>
    <icon>/opt/resources/console-app.ico</icon>
    <classPath>
        <mainClass>com.hazelcast.client.console.ClientConsoleApp</mainClass>
        <cp>%DIST_DIR%/lib/*.jar</cp>
    </classPath>
    <jre>
        <path>/opt/jdk8u265-b01-jre</path>
    </jre>
    <versionInfo>
        <fileVersion>%HAZELCAST_VERSION_WINDOWS%</fileVersion>
        <txtFileVersion>Hazelcast IMDG v. %HAZELCAST_VERSION%</txtFileVersion>
        <fileDescription>Launcher for Hazelcast member</fileDescription>
        <copyright>Hazelcast</copyright>
        <productVersion>%HAZELCAST_VERSION_WINDOWS%</productVersion>
        <txtProductVersion>Runs Hazelcast IMDG Member</txtProductVersion>
        <productName>Hazelcast IMDG</productName>
        <companyName>Hazelcast</companyName>
        <internalName>Hazelcast IMDG</internalName>
        <originalFilename>Hazelcast.exe</originalFilename>
    </versionInfo>
</launch4jConfig>
